    <materials>
        <material name="Blue" shader="diffuse-color">
            <parameter name="" vector3="0,0,1"/>
        </material>
        <material name="Red" shader="diffuse-color">
            <parameter name="" vector3="1,0,0"/>
        </material>
    </materials>
    <scenegraph>

            <camera/>
        </object>
        <object name="Blue cube" position="0,0,0" rotation="0,0,0" scale="1,1,1">
            <mesh material="Blue" primitive="cube"/> 
        </object>
        
        <object name="Red cube" position="2,2,2" rotation="0,45,0" scale="1.5,1.5,1.5">
            <mesh material="Red" primitive="cube"/>
